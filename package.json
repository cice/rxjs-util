{
  "name": "@zeit-dev/rxjs-util",
  "version": "1.0.0",
  "repository": "https://github.com/cice/rxjs-util",
  "description": "Utility classes, functions we commonly use across angular projects.",
  "author": "Marian Theisen <marian@cice-online.net>",
  "license": "MIT",
  "private": false,
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "devDependencies": {
    "@angular/compiler": ">= 8.0.0 < 10.0",
    "@angular/compiler-cli": ">= 8.0.0 < 10.0",
    "@angular/core": ">= 8.0.0 < 10.0",
    "ng-packagr": "^9.1.0",
    "rxjs": ">= 6.5.0",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.6",
    "typedoc-plugin-external-module-name": "^3.1.0",
    "typescript": ">=3.4 <3.8"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "lib/public-api.ts"
    }
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "publish-dist": "yarn publish dist --access public",
    "doc": "typedoc --out docs lib"
  },
  "peerDependencies": {
    "rxjs": ">= 6.5.0"
  }
}
